Number  EG_EZ_etrv_rssi_d "Sendestärke [%d]"						  { channel="homematic:HMIP-eTRV:ccu_neu:000397098A4A43:0#RSSI_DEVICE" }

Number  EG_HT_fsm_rssi "Empfangsstärke [%d]db"                                             { channel="homematic:HmIP-FSM:ccu_neu:000895699E669A:0#RSSI_PEER" }
