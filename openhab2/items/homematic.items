// Hier kommt der Wohnzimmerheizkörper
Group HK_WZ	 "Wohzimmerheizkörper"	[ "Thermostat" ]
	Number  EG_WZ_etrv 
		"Ist Temperatur WZ[%.1f °C]"          <temperature>
                (HK_WZ)
		[ "CurrentTemperature" ]      
		{ channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:1#ACTUAL_TEMPERATURE" }	
	Number  EG_WZ_etrv_s 
		"Soll Temperatur WZ[%.1f °C]"       <radiator> 
		(HK_WZ)
		[ "TargetTemperature" ] 
		{ channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:1#SET_POINT_TEMPERATURE",  knx="3/3/7" }
	String  HK_WZ_HCM
		"Mode WZ [%d]"
		(HK_WZ)
		[ "homekit:HeatingCoolingMode" ]
		{ channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:1#CONTROL_MODE" }

Number  EG_WZ_etrv_bat "Batterie Spannung [%.1f Volt]" <temperature>                      { channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:0#OPERATING_VOLTAGE" }
Number  EG_WZ_etrv_L "Ventilstellung [%d]"             <temperature>                      { channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:1#LEVEL" }
Number  EG_WZ_etrv_L_d "Ventilstellung [%d]"             <temperature>
Number  EG_WZ_etrv_rssi "Empfangsstärke [%d]"                                             { channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:0#RSSI_PEER" }
Number  EG_WZ_etrv_rssi_d "Sendestärke [%d]"                                              { channel="homematic:HMIP-eTRV:ccu_neu:000393C994D1A0:0#RSSI_DEVICE" }

Number  EG_EZ_etrv "Ist Temperatur EZ[%.1f °C]"          <temperature>                      { channel="homematic:HMIP-eTRV:ccu_neu:000397098A4A43:1#ACTUAL_TEMPERATURE" } 
Number  EG_EZ_etrv_s "Soll Temperatur EZ[%.1f °C]"       <radiator> [ "TargetTemperature" ] { channel="homematic:HMIP-eTRV:ccu_neu:000397098A4A43:1#SET_POINT_TEMPERATURE",  knx="3/2/3" }
Number  EG_EZ_etrv_bat "Batterie Spannung [%.1f Volt]" <temperature>                      { channel="homematic:HMIP-eTRV:ccu_neu:000397098A4A43:0#OPERATING_VOLTAGE" }
Number  EG_EZ_etrv_L "Ventilstellung [%d]"             <temperature>                      { channel="homematic:HMIP-eTRV:ccu_neu:000397098A4A43:1#LEVEL" }
