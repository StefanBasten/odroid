rule "dect200_1_ON"
        when
                Item TB_Erdgeschoss_Wohnzimmer changed from OFF to ON
        then
                sendCommand(Switch_Dect200_1,ON)
end

rule "dect200_1_OFF"
        when
                Item TB_Erdgeschoss_Wohnzimmer changed from ON to OFF
        then
                sendCommand(Switch_Dect200_1,OFF)
end

